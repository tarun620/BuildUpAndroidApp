                <TextView
                    android:id="@+id/tv_client_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Shashank Jain"
                    android:textColor="@color/black"
                    android:textSize="20sp"
                    android:fontFamily="@font/overpass_semibold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginStart="17dp"
                    android:layout_marginTop="10dp"/>

                <androidx.appcompat.widget.AppCompatImageButton
                    android:id="@+id/btn_change_address"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_group236_new"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:clickable="true"
                    android:focusable="true"
                    android:background="@color/white"
                    android:layout_marginEnd="6dp"
                    android:layout_marginTop="6dp"/>

                <TextView
                    android:id="@+id/tv_shipping_address"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="50 C ED Block, Madhuban Chowk, PitamPura, Delhi - 110034"
                    android:textColor="@color/black_text"
                    android:textSize="15sp"
                    android:fontFamily="@font/overpass"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/tv_client_name"
                    android:layout_marginTop="5dp"
                    android:layout_marginStart="17dp" />

                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/phone_icon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_call"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/tv_shipping_address"
                    android:layout_marginStart="17dp"
                    android:layout_marginBottom="12dp"
                    android:layout_marginTop="12dp"/>

                <TextView
                    android:id="@+id/tv_client_mobile_number"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="+91 8178600312"
                    android:textSize="15sp"
                    android:textColor="@color/black"
                    android:fontFamily="@font/overpass_semibold"
                    android:textFontWeight="600"
                    android:layout_marginStart="5dp"
